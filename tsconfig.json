{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "strict": false,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules",
    "plugins.ts"
  ]
}

